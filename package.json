    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@remix-run/express": "^1.8.2",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@types/exenv": "^1.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "exenv": "^1.2.2",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-scroll": "^1.8.8",
