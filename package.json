    "@types/react-dom": "^18.0.9",
    "@types/react-scroll": "^1.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
